{
  "Jwt": {
    "Key": "${JWT_KEY}",
    "Issuer": "${JWT_ISSUER}",
    "Audience": "${JWT_AUDIENCE}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApplicationInsights": {
    "InstrumentationKey": "${INSIGHTS_STRING}",
    "ConnectionString": "${INSIGHTS_STRING}"
  },
  "ConnectionStrings": {
    "AiLearnerConnection": "Server=${DB_SERVER};Initial Catalog=${DB_NAME};Persist Security Info=False;User ID=${ADMIN_ID};Password=${ADMIN_PASSWORD};MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
  },
  "OpenAI": {
    "ApiKey": "${OPENAI_APIKEY}"
  },
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "${AZURE_AD_DOMAIN}",
    "TenantId": "${AZURE_AD_TANANTID}",
    "ClientId": "${AZURE_AD_CLIENTID}",
    "CallbackPath": "/signin-oidc",
    "Scopes": ""
  }
}